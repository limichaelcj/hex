echo -e "  ${BG_GLD}BG_GLD  Gold   ${BG_D}  ${BG_YLW}BG_YLW  Yellow ${TF_N}"
echo -e "  ${BG_NAV}BG_NAV  Navy   ${BG_D}  ${BG_BLU}BG_BLU  Blue   ${TF_N}"
echo -e "  ${BG_ROY}BG_ROY  Royal  ${BG_D}  ${BG_PRP}BG_PRP  Purple ${TF_N}"
echo -e "  ${BG_TEA}BG_TEA  Teal   ${BG_D}  ${BG_CYN}BG_CYN  Cyan   ${TF_N}"
echo -e "  ${TC_BLK}${BG_SLV}BG_SLV  Silver ${BG_D}  ${BG_WHT}BG_WHT  White  ${TF_N}"
